{
  "name": "tree-sitter-vhs",
  "version": "0.1.0",
  "description": "Tree-sitter grammer for the CLI video recorder(.tape files)",
  "repository": "https://github.com/oxy2dev/tree-sitter-vhs.git",
  "license": "MIT",
  "author": {
    "name": "MD. Mouinul Hossain",
    "email": "mdmouinulhossainshawon@gmail.com",
    "url": "https://github.com/OXY2DEV/"
  },
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "vhs"
  ],
  "files": [
    "grammar.js",
    "tree-sitter.json",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "dependencies": {
    "node-addon-api": "^8.2.1",
    "node-gyp-build": "^4.8.2"
  },
  "devDependencies": {
    "prebuildify": "^6.0.1",
    "tree-sitter-cli": "^0.25.2"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.1"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "scripts": {
    "install": "node-gyp-build",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test": "node --test bindings/node/*_test.js"
  }
}
