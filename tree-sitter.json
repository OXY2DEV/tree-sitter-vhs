{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "vhs",
      "camelcase": "VHS",
      "title": "VHS",
      "scope": "source.vhs",
      "file-types": [
        "tape"
      ],
      "injection-regex": "^vhs$",
      "class-name": "TreeSitterVhs",
      "highlights": "./queries/highlights.scm"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Tree-sitter grammar for the CLI video recorder(.tape files)",
    "authors": [
      {
        "name": "MD. Mouinul Hossain",
        "email": "mdmouinulhossainshawon@gmail.com",
        "url": "https://github.com/OXY2DEV/"
      }
    ],
    "links": {
      "repository": "https://github.com/OXY2DEV/tree-sitter-vhs.git"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
